# Konfiguracja AI Service (Python)

# Środowisko (development / production)
ENV=development

# Port nasłuchu (domyślnie 8000)
PORT=8000

# Adres bazy danych (opcjonalnie, jeśli AI ma bezpośredni dostęp)
# DATABASE_URL=postgresql://user:password@localhost:5432/warehouse

# Parametry algorytmu optymalizacji
# Domyślna długość nowej sztangi (mm)
DEFAULT_BAR_LENGTH_MM=6500

# Minimalna długość ścinka uznawanego za użyteczny (mm)
MIN_USABLE_OFFCUT_MM=500

# Próg złomu - poniżej tej długości odpad jest utylizowany (mm)
SCRAP_THRESHOLD_MM=200
